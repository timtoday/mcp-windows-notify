# Windows通知配置
NOTIFICATION_TITLE=🤖 AI助手通知
NOTIFICATION_SOUND=true
NOTIFICATION_TIMEOUT=0
NOTIFICATION_ICON=info

# 通知行为配置
# NOTIFICATION_TIMEOUT=0 表示常驻通知，直到用户关闭
# NOTIFICATION_TIMEOUT=5000 表示5秒后自动消失
# NOTIFICATION_SOUND=true 开启声音提醒
# NOTIFICATION_SOUND=false 关闭声音提醒

# MCP服务器配置
MCP_SERVER_NAME=windows-notify
MCP_SERVER_VERSION=1.0.0
